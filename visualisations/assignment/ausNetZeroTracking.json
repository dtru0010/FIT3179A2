{
    "$schema": "https://vega.github.io/schema/vega-lite/v5.json",
    "title": "Title",
    "width": 800,
    "height": 450,
    "layer": [
      {
        "data": {
          "url": "https://raw.githubusercontent.com/dtru0010/FIT3179A2/refs/heads/main/data/assignment/aus_net_zero_2030_target_tracking_modified.csv",
          "format": {"type": "csv"}
        },
        "params": [
          {
            "name": "line_selection",
            "select": {
              "type": "point",
              "fields": ["Category"]
            },
            "bind": "legend"
          }
        ],
        "transform": [
          {"filter": "datum.Category != '2030 Target'"},
          {"filter": "datum.Category != '2050 Target'"}
        ],
        "mark": {"type": "line"},
        "encoding": {
          "x": {
            "field": "Year",
            "type": "quantitative",
            "title": "Year",
            "axis": {
              "labelAngle": 45,
              "format": "d"
            }
          },
          "y": {
            "field": "Emissions",
            "type": "quantitative",
            "title": "Emissions (Mt CO2-e)"
          },
          "color": {
            "field": "Category",
            "type": "nominal",
            "title": ""
          },
          "tooltip": [
            {
              "field":"Category",
              "type": "nominal",
              "title": "Relational Category:"
            },
            {
              "field": "Year",
              "type": "nominal",
            "title": "Year:"
            },
            {
              "field": "Emissions",
              "type": "quantitative",
              "title": "Emissions (Mt CO2-e):",
              "format": ".2f"
            }
          ],
          "opacity": {
            "condition": {"param": "line_selection", "value": 1},
            "value": 0.2
          }
        }
      },
      {
        "data": {
          "url": "https://raw.githubusercontent.com/dtru0010/FIT3179A2/refs/heads/main/data/assignment/aus_net_zero_2030_target_tracking_modified.csv",
          "format": {"type": "csv"}
        },
        "mark": {"type": "circle"},
        "transform": [
          {"filter": "datum.Category == '2030 Target' || datum.Category == '2050 Target'"}
        ],
        "encoding": {
          "x": {
            "field": "Year",
            "type": "quantitative",
            "title": "Year",
            "axis": {
              "labelAngle": 45,
              "format": "d"
            }
          },
          "y": {
            "field": "Emissions",
            "type": "quantitative",
            "title": "Emissions (Mt CO2-e)"
          },
          "color": {
            "field": "Category",
            "type": "nominal",
            "title": ""
          },
          "tooltip": [
            {
              "field":"Category",
              "type": "nominal",
              "title": "Relational Category:"
            },
            {
              "field": "Year",
              "type": "nominal",
              "title": "Year:"
            },
            {
              "field": "Emissions",
              "type": "quantitative",
              "title": "Emissions (Mt CO2-e):",
              "format": ".2f"
            }
          ],
          "size": {
            "value": 50
          }
        }
      },
      {
        "data": {
          "url": "https://raw.githubusercontent.com/dtru0010/FIT3179A2/refs/heads/main/data/assignment/aus_net_zero_2030_target_tracking_modified.csv",
          "format": {"type": "csv"}
        },
        "mark": {
          "type": "text",
          "dy": -20,
          "dx": -100,
